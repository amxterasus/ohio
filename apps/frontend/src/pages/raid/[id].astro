---
import Layout from '@/layouts/Layout.astro';
import Terminal from '@/components/cards/Terminal.astro';

export const prerender = false;

const { id } = Astro.params;
---

<Layout title={`Raid ${id}`}>
  <div class="container mx-auto px-4 py-8">
    <h1 class="text-center text-3xl font-bold mb-4">Raid {id}</h1>
    <p class="text-center mb-6">This is a placeholder for raid details.</p>
    <Terminal />
  </div>
</Layout>
